  /*  Beans models  */
  "Beans": {
    /*
     buttons
    */
    "Button": {
      "tag": "button",
	  "displayName": "Button",
      "label": "Button",
      "description": "button| Defines a button. Buttons are an essential way to interact with and navigate through an app, and should clearly communicate what action will occur after the user taps them.<br/>Buttons can consist of text and/or an icon, and can be enhanced with a wide variety of attributes.<br/>You can add event components to buttons to handle button clicks. <br/> For more information: <a href='https://ionicframework.com/docs/components/#buttons' target='_blank'>Buttons</a>",
      "group": "Buttons",
      "icon16": "button_color_16x16.png",
      "icon32": "button_color_32x32.png",
      "properties": {
        "ButtonFormType" : false,
        "ButtonType": "ion-button",
        "MenuMode": false,
        "IonColor": false,
        "IonShape": false,
        "IonOutline": false,
		"IonClear": false,
        "IonSize": false,
        "IconPosition": false,
        "IconOnly": false
      }
    },

    "SubmitButton": {
      "tag": "button",
	  "displayName": "SubmitButton",
      "label": "Submit Button",
      "description": "button| Defines a submit button for a Form. Use a 'onSubmit' event on the Form component to handle the Submit event.<br /><br />You can use the 'AutoDisabled' property of the Submit button to disable it if the form is not valid.",
      "group": "Forms",
      "icon16": "button_color_16x16.png",
      "icon32": "button_color_32x32.png",
      "properties": {
        "ButtonFormType" : "submit",
        "ButtonType": "ion-button",
        "MenuMode": false,
        "IonColor": false,
        "IonShape": false,
        "IonOutline": false,
        "IonSize": false,
        "IconPosition": false,
        "IconOnly": false,
		    "AutoDisable": {
      		"label": "Auto disable",
      		"description": "Disable button if it's associated form is not valid",
      		"category": "@Forms",
      		"value": "true",
      		"values": [
        		false,
        		"true"
      		]
		}
      }
    },

    "ResetButton": {
      "tag": "button",
	  "displayName": "ResetButton",
      "label": "Reset Button",
      "description": "button| Defines a reset button for a Form. The reset button will clear all fields of the Form",
      "group": "Forms",
      "icon16": "button_color_16x16.png",
      "icon32": "button_color_32x32.png",
      "properties": {
        "ButtonFormType" : "reset",
        "ButtonType": "ion-button",
        "MenuMode": false,
        "IonColor": false,
        "IonShape": false,
        "IonOutline": false,
        "IonSize": false,
        "IconPosition": false,
        "IconOnly": false
      }
    },
    
    "FabButton": {
      "tag": "button",
	  "displayName": "FabButton",
      "label": "FAB Button",
      "description": "button| Defines a floating button. FABs (Floating Action Buttons) are standard material design components.<br/>They are shaped as a circle that represents a promoted action. When pressed, it may contain more related actions.<br/>FABs as its name suggests are floating over the content in a fixed position. You can add icons in the button to represent a given function. <br/> For more information: <a href='https://ionicframework.com/docs/components/#fabs' target='_blank'>FABs</a>",
      "group": "Buttons",
      "icon16": "fab_16x16.png",
      "icon32": "fab_32x32.png",
      "properties": {
        "ButtonType": "ion-fab",
        "IonColor": false,
        "Size": {
          "label": "size",
          "description": "Specifies FAB size",
          "category": "@Properties",
          "values": [
            false,
            "mini"
          ]
        }
      }
    },


    "FabContainer": {
      "tag": "ion-fab",
	  "displayName": "FabContainer",
      "label": "FAB Container",
      "description": "ion-fab| Defines a container for floating buttons. Place your FAB buttons (Floating Action Button) in this container to make them non scrollable with content.",
      "group": "Buttons",
      "icon16": "container_fab_16x16.png",
      "icon32": "container_fab_32x32.png",
      "properties": {
        "HorzPosition": {
          "label": "Horizontal Position",
          "description": "Specifies the horizontal position",
          "category": "@Properties",
          "values": [
            false,
            "left",
            "right"
          ]
        },
        "VertPosition": {
          "label": "Vertical Position",
          "description": "Specifies the vertical position",
          "category": "@Properties",
          "values": [
            false,
            "top",
            "bottom"
          ]
        },
        "Edge": {
          "label": "Edge",
          "description": "Specifies if the FAB must be positioned on the edge",
          "category": "@Properties",
          "values": [
            false,
            "edge"
          ]
        }
      }
    },

    "Icon": {
      "tag": "ion-icon",
	  "displayName": "Icon",
      "label": "Icon",
      "description": "ion-icon| Defines an icon. More than 700+ Ionicons icons we all come to know and love.<br/>Can be used in buttons.<br />See link for <a href='https://ionicframework.com/docs/ionicons/' target='_blank'>icon descriptions</a>",
