    "Slides": {
      "tag": "ion-slides",
	  "displayName": "Slides",
      "label": "Slides container",
      "description": "ion-slides| Defines slides. Slides make it easy to create galleries, tutorials, and page-based layouts. Slides take a number of configuration options on the &lt;ion-slides&gt; component.<br/>The Slides component is a multi-section container. Each section can be swiped or dragged between. It contains any number of Slide components. <br/> For more information: <a href='https://ionicframework.com/docs/components/#slides' target='_blank'>Slides</a>",
      "group": "Slides",
      "icon16": "slides_color_16x16.png",
      "icon32": "slides_color_32x32.png",
      "properties": {
        "AutoPlay": {
          "mode": "script",
          "attr": "[autoplay]",
          "label": "Autoplay",
          "description": "Delay between transitions (in milliseconds). If this parameter is not passed, autoplay is disabled. Default does not have a value and does not autoplay. Default: null",
          "category": "@Properties",
          "value": ""
        },
        "Direction": {
          "mode": "script",
          "attr": "[direction]",
          "label": "Direction",
          "description": "Swipe direction: 'horizontal' or 'vertical'. Default: horizontal.",
          "category": "@Properties",
          "value": "horizontal",
          "values": [
            "horizontal",
            "vertical"
          ]
        },
        "Pager": {
          "mode": "script",
          "attr": "[pager]",
          "label": "Pager",
          "description": "If true, show the pager. (Little dots for each page)",
          "category": "@Properties",
          "value": "true",
          "values": [
            "true",
            "false"
          ]
        },
        "PaginationType": {
          "mode": "script",
          "attr": "[paginationType]",
          "label": "Pagination type",
          "description": "Type of pagination. Possible values are: bullets, fraction, progress. Default: bullets. (Note that the pager will not show unless pager input is set to true).",
          "category": "@Properties",
          "value": "bullets",
          "values": [
            "bullets",
            "fraction",
            "progress"
          ]
        },
        "parallax": {
          "mode": "script",
          "attr": "[parallax]",
          "label": "Parallax",
          "description": "If true, allows you to use 'parallaxed' elements inside of slider",
          "category": "@Properties",
          "value": "false",
          "values": [
            "true",
            "false"
          ]
        },
        "SlidesPerView": {
		  "mode": "script",
          "attr": "[slidesPerView]",
          "label": "Slides per view",
          "description": "Slides per view. Slides visible at the same time. Default, 1",
