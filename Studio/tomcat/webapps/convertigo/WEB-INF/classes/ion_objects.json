		  "properties": {
    		"closeDuration": {
    			"attr":"closeDuration",
    			"label": "Close Duration",
    			"description": "How many milliseconds it takes to close the refresher.",
    			"category": "@Properties",
    			"value": 280
    		},
			  "enabled": {
    			"attr":"enabled",
    			"label": "Enabled",
    			"description": "If the refresher is enabled or not. This should be used in place of an ngIf.",
    			"category": "@Properties",
    			"value": true,
          "values":[
            true,
            false
          ]
    		},
			  "pullMax": {
    			"attr":"pullMax",
    			"label": "PullMax",
    			"description": "The maximum distance of the pull until the refresher will automatically go into the refreshing state. By default, the pull maximum will be the result of pullMin + 60.",
    			"category": "@Properties"
    		},
			  "pullMin": {
    			"attr":"pullMin",
    			"label": "PullMin",
    			"description": "The min distance the user must pull down until the refresher can go into the refreshing state.",
    			"category": "@Properties",
    			"value": 60
    		},
			  "snapbackDuration": {
    			"attr":"snapbackDuration",
    			"label": "SnapbackDuration",
    			"description": "How many milliseconds it takes the refresher to to snap back to the refreshing state.",
    			"category": "@Properties",
    			"value": 280
    		}
