      "icon16": "content_color_16x16.png",
      "icon32": "content_color_32x32.png",
      "properties": {
        "IonPadding": "padding",
        "Fullscreen": {
          "attr": "fullscreen",
          "label": "full screen",
          "description": "Specifies a fullscreen scrollable content",
          "category": "Attributes",
          "value": false,
          "values": [
            false,
            "false",
            "true"
          ]
        }
      }
    },
    
    "Footer": {
      "tag": "ion-footer",
	  "displayName": "Footer",
