    "GoogleMap" : {
      "tag": "agm-map",
	  "displayName": "GoogleMap",
      "label": "Google Map",
      "description": "agm-map| Defines a Google Map. Displays a Google Map able to be positioned on a given latitude and longitude.<br /><br />You can insert in the Map any number of 'Marker' objects to mark a positions on the map, and within 'Marker' an 'InfoWindow' object to display more information on a given marker.<br /><br />'InfoWindow' objects can have any type of HTML information inside to display formated information when the user clicks on a mark, including action buttons.<br /><br />All coordinates are as jSON 'number' type. To be sure to generate 'number' types within Sequences be sure to add an 'Attribute type=double' step to your 'Element' steps",
      "group": "Google Map",
      "icon16": "gmap_color_16x16.png",
      "icon32": "gmap_color_32x32.png",
      "properties": {
        "Latitude": {
          "mode": "script",
          "attr":"[latitude]",
          "label": "Latitude",
          "description": "Number : The map will be centered at this latitude. Must be a 'number' jSON type (ex: 51.678418)",
          "category": "@Coordinates",
          "value": "48.6969459"
        },
        "Longitude": {
          "mode": "script",
          "attr":"[longitude]",
          "label": "Longitude",
          "description": "Number : The map will be centered at this longitude. Must be a 'number' jSON type (ex: 7.809007)",
          "category": "@Coordinates",
          "value": "2.1775981"
        },
        "Height": {
          "mode": "script",
          "attr":"[style.height]",
          "label": "Display Height",
          "description": "The Height in pixels (px) or in percentage (%) the map uses on the screen.",
          "category": "@Display",
          "value": "'100%'"
        },
        "Zoom": {
          "mode": "script",
          "attr":"[zoom]",
          "label": "Zoom level",
          "description": "The zoom level of the map. The default zoom level is 8.",
          "category": "@Display",
          "value": "12"
        }
      },
      "events": {
        "onBoundsChange": {
          "attr":"(boundsChange)",
          "description": "This event is fired when the viewport bounds have changed."
        },
        "onCenterChange": {
          "attr":"(centerChange)",
          "description": "This event is fired when the map center changes."
        },
        "onIdle": {
          "attr":"(idle)",
          "description": "This event is fired when the map becomes idle after panning or zooming."
        },
        "onMapClick": {
          "attr":"(mapClick)",
          "description": "This event gets emitted when the user clicks on the map (but not when user click on a marker or infoWindow)."
        },
        "onMapDblClick": {
          "attr":"(mapDblClick)",
          "description": "This event gets emitted when the user double-clicks on the map (but not when they click on a marker or infoWindow)."
        },
        "onMapReady": {
          "attr":"(mapReady)",
          "description": "This event is fired when the google map is fully initialized. You get the google.maps.Map instance as a result of this EventEmitter."
        },
        "onMapRightClick": {
          "attr":"(mapRightClick)",
          "description": "This event gets emitted when the user right-clicks on the map (but not when they click on a marker or infoWindow)."
        },
        "onZoomChange": {
          "attr":"(zoomChange)",
          "description": "This event is fired when the zoom level has changed."
        }
      }
    },
    
    "MapMarker" : {
      "tag": "agm-marker",
	  "displayName": "MapMarker",
      "label": "Map Marker",
      "description": "agm-marker| Defines a marker on a Google Map. A 'Marker' is displayed on a map to mark to a specific point. Several markers can be displayed on a map, just add them in your 'GoogleMap' component.<br /><br /> You can add markers using a 'ForEach' directive to display markers from a collection.",
