      "properties": {
        "IconName": "add",
        "ItemPosition": false,
        "IonColor": false,
        "RangeLeft": false,
        "RangeRight": false
      }
    },


    "ButtonSet": {
      "tag": "ion-buttons",
	  "displayName": "ButtonSet",
      "label": "Button Set",
      "description": "ion-buttons| Defines a set of buttons to be used within other components. For example, buttons can be added to a list item or a navigation bar.<br/>You can define whether buttons are aligned left (start) or right (end). <br/> For more information: <a href='https://ionicframework.com/docs/components/#buttons-in-components' target='_blank'>Button Set</a>",
      "group": "Buttons",
      "icon16": "buttonset_color_16x16.png",
      "icon32": "buttonset_color_32x32.png",
      "properties": {
        "SetPosition": {
          "label": "position",
          "description": "Specifies the position",
          "category": "Attributes",
          "value": false,
          "values": [
            false,
            "start",
            "end",
            "left",
            "right"
          ]
        }
      }
    },
	
    "Content": {
      "tag": "ion-content",
	  "displayName": "Content",
      "label": "Content",
      "description": "ion-content| Defines a content area. This is the main space where other commponents will be inserted. You can use the 'padding' property to automatically define some margins in the 'content' component",
