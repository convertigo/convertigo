{
	"name": "convertigo-studio-web",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && node post-build.js",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-check --tsconfig ./jsconfig.json --ignore \"src/lib/studio/**/*\" --watch",
		"test": "npm run test:integration && npm run test:unit",
		"lint": "prettier --plugin prettier-plugin-svelte --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"update:bezels": "node scripts/update-bezels.mjs",
		"generate:thumbnails": "node scripts/generate-thumbnails.mjs",
		"check:admin": "svelte-check --no-tsconfig --ignore \"src/lib/studio/**,src/routes/(app)/admin/certificates/**,src/routes/(app)/admin/projects/**,src/routes/(app)/admin/roles/**,src/routes/(app)/admin/symbols/**\""
	},
	"overrides": {
		"svelte-tiny-virtual-list": {
			"@types/svelte": "^5.0.0",
			"svelte": "^5.43.14"
		}
	},
	"devDependencies": {
		"@fontsource-variable/inter": "^5.2.8",
		"@ianvs/prettier-plugin-sort-imports": "^4.7.0",
		"@iconify-json/mdi": "^1.2.3",
		"@iconify/svelte": "^5.1.0",
		"@playwright/test": "^1.56.1",
		"@skeletonlabs/skeleton": "^4.5.0",
		"@skeletonlabs/skeleton-svelte": "^4.5.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.49.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "0.5.10",
		"@tailwindcss/typography": "0.5.19",
		"@types/node": "^24.10.1",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"postcss-nesting": "^13.0.2",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"sharp": "^0.34.5",
		"svelte": "^5.43.14",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"unplugin-icons": "^22.5.0",
		"vite": "^7.2.4",
		"vitest": "^4.0.12"
	},
	"type": "module",
	"dependencies": {
		"@flaticon/flaticon-uicons": "^3.3.1",
		"@floating-ui/dom": "^1.7.4",
		"@internationalized/date": "^3.10.0",
		"@tailwindcss/vite": "^4.1.17",
		"@xyflow/svelte": "^1.4.2",
		"@zag-js/tree-view": "^1.29.0",
		"apexcharts": "^5.3.6",
		"bits-ui": "^2.14.4",
		"caniuse-lite": "^1.0.30001756",
		"chart.js": "^4.5.1",
		"elkjs": "^0.11.0",
		"fast-xml-parser": "^5.3.2",
		"fs-extra": "^11.3.2",
		"html-entities": "^2.6.0",
		"marked": "^17.0.1",
		"monaco-editor": "^0.55.1",
		"svelte-persisted-state": "^1.2.0",
		"svelte-persisted-store": "^0.12.0",
		"svelte-tiny-virtual-list": "^3.0.1"
	}
}
