{
  "name": "Convertigo-template-app",
  "author": "Convertigo & Ionic Framework",
  "homepage": "http://www.convertigo.com/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build --enableLint false --generateSourceMap false",
    "ionic:build": "ionic-app-scripts build --enableLint false",
    "ionic:build:prod":"node --max_old_space_size=8192 ./node_modules/@ionic/app-scripts/bin/ionic-app-scripts.js  build --prod --enableLint false",
    "ionic:serve:eval": "ionic-app-scripts serve --enableLint false --sourceMapType eval",
    "ionic:serve:nosourcemap": "ionic-app-scripts serve --enableLint false --generateSourceMap false",
    "ionic:serve": "ionic-app-scripts serve --enablelint false"
  },
  "dependencies": {
    "@angular/animations": "5.2.11",
    "@angular/common": "5.2.11",
    "@angular/compiler": "5.2.11",
    "@angular/compiler-cli": "5.2.11",
    "@angular/core": "5.2.11",
    "@angular/forms": "5.2.11",
    "@angular/http": "5.2.11",
    "@angular/platform-browser": "5.2.11",
    "@angular/platform-browser-dynamic": "5.2.11",
    "@ionic-native/core": "~4.20.0",
    "@ionic-native/splash-screen": "~4.20.0",
    "@ionic-native/status-bar": "~4.20.0",
    "@ionic/storage": "2.2.0",
    "ionic-angular": "3.9.9",
    "ionicons": "3.0.0",
    "rxjs": "6.3.3",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.29",
    "@ngx-translate/core":"9.0.2",
    "@ngx-translate/http-loader":"2.0.1",
    "animate.css": "3.7.0",  
    "c8osdkangular": "3.0.3",
    "c8ocaf": "1.1.10"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.2.4",
    "typescript": "~2.6.2"
  },
  "description": "MyApp: An Ionic project",
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard"
  ],
  "config": {
    "ionic_copy": "./config/copy.config.js",
    "ionic_watch": "./config/watch.config.js",
    "ionic_www_dir": "../DisplayObjects/mobile"
  },
  "cordovaPlatforms": []
}