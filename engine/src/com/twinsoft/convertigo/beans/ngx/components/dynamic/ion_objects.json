    "Refresher": {
  		"tag": "ion-refresher",
		"displayName": "Refresher",
    	"label": "Refresher",
        "description": [
"ion-refresher| Defines a <i>Refresher</i> component.<br/>",
"This component provides pull-to-refresh functionality on a <i>Content</i> component.<br/>",
"Place the <i>Refresher</i> as the first child of your <i>Content</i>.<br/>",
"Pages can then listen to the refresher's various output events. The refresh output event is fired when the user has pulled down far enough to kick off the refreshing process.<br/>",
" Once the async operation has completed and the refreshing should end, call complete().<br/>",
" For more information: <a href='https://ionicframework.com/docs/v3/api/components/refresher/Refresher/'>Refresher</a>."],
    	"group": "Forms",
    	"icon16": "refresher_color_16x16.png",
    	"icon32": "refresher_color_32x32.png",
		"properties": {
		    "Disabled": "false",
		    "IonSlot": "fixed",
			"CloseDuration": {
    		   "attr":"close-duration",
    		   "label": "Close duration",
    		   "description": "Time it takes to close the refresher. Does not apply when the refresher content uses a spinner, enabling the native refresher.",
    		   "category": "@Properties",
    		   "value": "280ms"
    		},
    		"PullFactor": {
    		   "attr": "pull-factor",
    		   "label": "Pull factor",
    		   "description": "How much to multiply the pull speed by. To slow the pull animation down, pass a number less than 1. To speed up the pull, pass a number greater than 1. The default value is 1 which is equal to the speed of the cursor.",
    		   "category": "@Properties",
    		   "type": "number",
    		   "value": "1"
    		},
    		"PullMax": {
    		   "attr": "pull-max",
    		   "label": "Pull max",
    		   "description": "The maximum distance of the pull until the refresher will automatically go into the refreshing state. Defaults to the result of pullMin + 60. Does not apply when the refresher content uses a spinner, enabling the native refresher.",
    		   "category": "@Properties",
    		   "type": "number",
    		   "value": false
    		},
    		"PullMin": {
    		   "attr": "pull-min",
    		   "label": "Pull min",
    		   "description": "The minimum distance the user must pull down until the refresher will go into the refreshing state. Does not apply when the refresher content uses a spinner, enabling the native refresher.",
    		   "category": "@Properties",
    		   "type": "number",
    		   "value": "60"
    		},
			"SnapbackDuration": {
    			"attr":"snapback-duration",
    			"label": "Snapback duration",
    			"description": "Time it takes the refresher to to snap back to the refreshing state. Does not apply when the refresher content uses a spinner, enabling the native refresher.",
    			"category": "@Properties",
    			"value": "280ms"
    		}
    	},
      	"events": {
	        "ionPull": {
	          "attr":"(ionPull)",
	          "description": "This event is emitted while the user is pulling down the content and exposing the refresher."
	        },
	        "ionRefresh": {
	          "attr":"(ionRefresh)",
	          "description": "Emitted when the user lets go of the content and has pulled down further than the `pullMin` or pulls the content down and exceeds the pullMax. Updates the refresher state to `refreshing`. The `complete()` method should be called when the async operation has completed."
	        },
	        "ionStart": {
	          "attr":"(ionStart)",
	          "description": "This event is emitted when the user begins to start pulling down."
	        }
      	}
    },
    
