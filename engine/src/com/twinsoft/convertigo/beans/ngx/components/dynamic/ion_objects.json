					"category": "@mocking options login",
					"type": "string",
					"value": "my_id_token"
				},
				"mockedResponseS": {
					"label": "Mocked server auth code login",
					"description": "The mocked response server auth code for the GooglePlus login action in case of running not supported platform. Must be a string.",
					"category": "@mocking options login",
					"type": "string",
					"value": "12"
				},
				"mockedResponseA": {
