{
  /*  Property models   */
  "Props": {
	
	"ControlName": {
      "label": "Control name",
      "description": "The associated declared control name.",
      "category": "@Control",
      "attr": "name",
      "value": ""
	},
	
    "DoubleBinding": {
      "attr": "[(ngModel)]",
      "label": "Binding",
      "description": "The object or variable to bind with the control.",
      "category": "@Control",
      "mode": "script",
      "value": false
    },
    
    "Disabled": {
      "attr": "disabled",
      "label": "Disabled",
      "description": "If true the component will be disabled, preventing the the user from changing the value.",
      "category": "@Properties",
      "type": "boolean",
      "value": "false",
      "values": [
        "false",
        "true"
      ]
    },
	
    "RangeLeft": {
      "label": "Range Left",
      "description": "Labels can be placed on either side of the range by adding the range-left or range-right property to the element. The element doesn't have to be an ion-label, it can be added to any element to place it to the left or right of the range.",
      "category": "@Range Labels",
      "value": false,
      "values": [
        false,
        "range-left"
      ]
    },

    "RangeRight": {
      "label": "Range Right",
      "description": "Labels can be placed on either side of the range by adding the range-left or range-right property to the element. The element doesn't have to be an ion-label, it can be added to any element to place it to the left or right of the range.",
      "category": "@Range Labels",
      "value": false,
      "values": [
        false,
        "range-right"
      ]
    },

    "ButtonType": {
      "attr": "button-type",
      "label": "Button type",
      "description": "Specifies ionic button style.",
      "category": "@Properties",
      "value": "button", // optional value (default is false)
      "values": [ // possible values (false means do no set)
        false,
        "button"
      ]
    },
    
    "ButtonFormType": {
      "attr": "type",
      "label": "Button form type",
      "description": "Specifies form's button type.",
      "category": "@Forms",
      "values": [
        false,
        "reset",
        "submit",
        "button"
      ]
    },
    
    "IconOnly": {
      "label": "Icon only",
      "description": "Set if we only show the icon.",
      "category": "@Properties",
      "values": [
        false,
        "icon-only"
      ]
    },
    
    "MenuMode": {
      "label": "Menu mode",
      "description": "Specifies menu mode.",
      "category": "@Properties",
